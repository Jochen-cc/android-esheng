<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qcyg.jochen.esheng"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.qcyg.jochen.esheng.StartActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".leading.LeadActivity" />
        
        <activity android:name=".webView.WebViewActivity" />
        <activity android:name=".webView.LocalMarkdownActivity" />
        <activity android:name=".webView.OnlineMarkdownActivity" />
        <activity android:name=".webView.MarkdownThemesActivity" />
        <activity
            android:name=".webView.MarkdownDataActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".photoedit.CropImageActivity" />
        <activity android:name=".photoedit.PhotoEditActivity" />
    </application>

</manifest>